{
  "name": "shiftbook-db-deployer",
  "description": "Database deployer module for ShiftBook application",
  "version": "1.0.0",
  "dependencies": {
    "hdb": "^0",
    "@sap/hdi-deploy": "^5"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node node_modules/@sap/hdi-deploy/deploy.js --use-hdb --parameter com.sap.hana.di.table/try_fast_table_migration=true"
  }
}